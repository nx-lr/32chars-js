      "start": "nodemon --exec babel-node index.js",
      "flow": "flow",
      "test": "echo \"Error: no test specified\" && exit 1"
    },
    "directories": {
      "doc": "docs",
      "test": "tests"
    },
    "engines": {
      "vscode": "^1.58.0"
    },
    "categories": [
      "Programming Languages",
      "Themes"
    ],
    "contributes": {
